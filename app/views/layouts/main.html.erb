<!DOCTYPE html>
<html>
<head>
  <title>Toucan | <%= @page_title || "Home" %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <script src="https://cdn.socket.io/socket.io-1.3.5.js"></script>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  
  
  <%= csrf_meta_tags %>
</head>
<body class = "main_body wall" style = "background-image: url(<%= asset_path 'pat.png'%>)">


<%= render(:partial => "layouts/nav")%>



<%= yield %>
<%= render(:partial => "report") %>
<%= render(:partial => "notification_box") %>

</body>
<% if account_signed_in? %>
<%= subscribe_to account_path(current_account) %>

<script type="text/javascript">
var notification_box = new NotificationBox("test_notification", 20, 20);
var current_user_id = function(){
	return ($("#current_user_id_container_span").attr("current-user-id"));
}
</script>
<% end %>
</html>
